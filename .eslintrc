{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node": true
    },
    "rules": {
        "no-console": [
            "off"
        ],
        "semi": [
            "error",
            "always"
        ],
        "indent": [
            "error",
            4
        ]
    }
}